{
  "version": 2,
  "buildCommand": "cd court-kiosk/frontend && npm run build",
  "outputDirectory": "court-kiosk/frontend/build",
  "installCommand": "cd court-kiosk/frontend && npm install",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://court-kiosk-backend.vercel.app"
  }
}
